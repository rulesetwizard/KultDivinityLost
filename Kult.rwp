function getAttributeValue()
	local nodeWin = window.windowlist.window.getDatabaseNode();
	local nodeAdvDis = window.getDatabaseNode();
	local attr = nodeAdvDis.getChild("attributevalue");
	if attr then 
		local nodeAttr = nodeWin.getChild(attr.getValue());
		if nodeAttr then
			local attrValue = nodeAttr.getValue();
			if attrValue then
				return attrValue;
			end
		end
	end;
	return 0;
end</LUACode>
