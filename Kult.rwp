    <LUACode>
function onInit()
	super.onInit();
	setStability();
end

function onValueChanged()
	setStability();
end

function setStability()
	local stabilityDesc = "";
	local stabilityHelp = "";
	local stability = tonumber(getDatabaseNode().getValue());

	if stability and stability &gt;= 7 then 
		stabilityDesc = Interface.getString("CRITICAL_STRESS");
		stabilityHelp = Interface.getString("CRITICAL_STRESS_HELP");
	else
		if stability and stability &gt;= 4 then 
			stabilityDesc = Interface.getString("SERIOUS_STRESS");
			stabilityHelp = Interface.getString("SERIOUS_STRESS_HELP");
		else
			if stability and stability &gt;= 2 then 
				stabilityDesc = Interface.getString("MODERATE_STRESS");
				stabilityHelp = Interface.getString("MODERATE_STRESS_HELP");
			end
		end
	end;

	window.lblStabilityDesc.setValue(stabilityDesc);
	window.lblStabilityHelp.setTooltipText(stabilityHelp);
end

