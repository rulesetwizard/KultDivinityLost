end

function calcWounds()
	local nWounds = 0;
	for i=1,4,1 do
		if manager_kult.trim(DB.getValue(getDatabaseNode(), "wound" .. i, '')) ~= "" then 
			nWounds = nWounds + 1;
		end
	end
	wounds.setValue(nWounds);
	--DB.setValue(getDatabaseNode(), "wounds", number, nWounds);
end</LUACode>
